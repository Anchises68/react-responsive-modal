{
  "name": "react-responsive-modal",
  "version": "1.9.1",
  "description": "A simple responsive react modal",
  "main": "lib/modal.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "build:watch": "npm run build -- --watch",
    "build:examples": "browserify examples/app.js -o examples/app.bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "lint": "eslint src docs/src",
    "test": "npm run lint && npm run build && npm run size",
    "size": "size-limit",
    "format": "prettier --single-quote --trailing-comma es5 --write \"src/**/*.js\" \"docs/src/**/*.{js,css}\""
  },
  "keywords": [
    "react",
    "responsive",
    "modal",
    "flex"
  ],
  "repository": "git+https://github.com/pradel/react-responsive-modal.git",
  "author": "LÃ©o Pradel",
  "bugs": {
    "url": "https://github.com/pradel/react-responsive-modal/issues"
  },
  "homepage": "https://github.com/pradel/react-responsive-modal#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "no-scroll": "^2.1.0",
    "prop-types": "^15.6.0",
    "react-jss": "^7.2.0",
    "react-minimalist-portal": "^2.1.1",
    "react-transition-group": "^1.2.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-jsx-import": "^0.0.1",
    "eslint-plugin-react": "^7.4.0",
    "prettier": "^1.7.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "size-limit": "^0.12.1"
  },
  "size-limit": [
    {
      "path": "lib/modal.js",
      "limit": "19 KB"
    }
  ],
  "license": "MIT"
}
